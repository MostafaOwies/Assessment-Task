    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_navs.xml"
    app:startDestination="@id/userData">

    <fragment
        android:id="@+id/userData"
        android:name="com.example.assessmenttask.presentation.userdata.UserData"
        android:label="fragment_user_data"
        tools:layout="@layout/fragment_user_data" >
        <action
